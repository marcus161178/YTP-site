<h1>Listing subcategories</h1>



<% @subcategories.each do |subcategory| %>

    
    <% subcategory.categories.each do |category| %>
    <%= subcategory.name %>
     <%= subcategory.url %>
    <%= category.name %>
    <%= link_to 'Show', subcategory %>
    <%= link_to 'Edit', edit_subcategory_path(subcategory) %>
    <%= link_to 'Destroy', subcategory, method: :delete, data: { confirm: 'Are you sure?' } %>
    <br />
      <% end %>

<% end %>



<%= link_to 'New Subcategory', new_subcategory_path %>
